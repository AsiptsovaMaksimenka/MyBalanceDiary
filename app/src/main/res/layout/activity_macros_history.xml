<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MacrosHistory">


    <com.jjoe64.graphview.GraphView
        android:id="@+id/graph"
        android:layout_width="354dp"
        android:layout_height="362dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="289dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonLast7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="271dp"
        android:layout_marginBottom="227dp"
        android:backgroundTint="@android:color/darker_gray"
        android:text="Past 7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonLast30"
        android:layout_width="97dp"
        android:layout_height="46dp"
        android:layout_marginTop="456dp"
        android:layout_marginBottom="229dp"
        android:backgroundTint="@android:color/darker_gray"
        android:text="Past 30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/buttonAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="274dp"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="227dp"
        android:backgroundTint="@android:color/darker_gray"
        android:text="All"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Proteinsinfo"
        android:layout_width="328dp"
        android:layout_height="42dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="509dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="170dp"
        android:gravity="center"
        android:text=" "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.857" />


    <ImageView
        android:id="@+id/blue"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="528dp"
        android:layout_marginEnd="358dp"
        android:src="@drawable/blue"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/CarbsInfo"
        android:layout_width="324dp"
        android:layout_height="42dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="572dp"
        android:layout_marginBottom="118dp"
        android:gravity="center"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/red"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="583dp"
        android:layout_marginEnd="358dp"
        android:layout_marginBottom="124dp"
        android:src="@drawable/red"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        android:visibility="invisible"/>

    <TextView
        android:id="@+id/FatsInfo"
        android:layout_width="324dp"
        android:layout_height="45dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="621dp"
        android:layout_marginBottom="69dp"
        android:gravity="center"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.821"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/green"
        android:layout_width="21dp"
        android:layout_height="22dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="635dp"
        android:layout_marginEnd="361dp"
        android:layout_marginBottom="74dp"
        android:src="@drawable/green"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:visibility="invisible"/>

    <ImageView
        android:id="@+id/achive"
        android:layout_width="55dp"
        android:layout_height="33dp"
        android:layout_marginStart="192dp"
        android:layout_marginTop="648dp"
        android:layout_marginBottom="28dp"
        android:src="@drawable/c"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>