<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CaloriesHistory">


    <com.jjoe64.graphview.GraphView
        android:id="@+id/graph"
        android:layout_width="354dp"
        android:layout_height="362dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="289dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonLast7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="271dp"
        android:layout_marginBottom="227dp"
        android:backgroundTint="@android:color/darker_gray"
        android:text="Past 7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonLast30"
        android:layout_width="97dp"
        android:layout_height="46dp"
        android:layout_marginTop="456dp"
        android:layout_marginBottom="229dp"

        android:backgroundTint="@android:color/darker_gray"
        android:text="Past 30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/buttonAll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="274dp"
        android:layout_marginTop="456dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="227dp"
        android:backgroundTint="@android:color/darker_gray"
        android:text="All"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/CaloriesSummary"
        android:layout_width="336dp"
        android:layout_height="35dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="533dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="163dp"
        android:text=" "
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/blue"
        android:layout_width="22dp"
        android:layout_height="20dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="540dp"
        android:layout_marginEnd="366dp"
        android:layout_marginBottom="171dp"
        android:src="@drawable/blue"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/CaloriesPercent"
        android:layout_width="337dp"
        android:layout_height="34dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="590dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="107dp"
        android:text=""
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/green"
        android:layout_width="21dp"
        android:layout_height="22dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="599dp"
        android:layout_marginEnd="366dp"
        android:layout_marginBottom="110dp"
        android:src="@drawable/green"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/achive"
        android:layout_width="85dp"
        android:layout_height="55dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="648dp"
        android:layout_marginBottom="28dp"
        android:src="@drawable/c"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


</androidx.constraintlayout.widget.ConstraintLayout>