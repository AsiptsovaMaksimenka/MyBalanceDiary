<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ProgressBar
        android:id="@+id/fats_progress"
        style="@style/CircularDeterminateProgressBar"
        android:layout_width="165dp"
        android:layout_height="127dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.082"
        tools:progress="0" />

    <TextView
        android:id="@+id/text_view_fats"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="44dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/fats_progress"
        app:layout_constraintEnd_toEndOf="@+id/fats_progress"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/fats_progress"
        app:layout_constraintTop_toTopOf="@+id/fats_progress"
        app:layout_constraintVertical_bias="0.018"
        tools:text="0%" />


    <ProgressBar
        android:id="@+id/carbs_progress"
        style="@style/CircularDeterminateProgressBar"
        android:layout_width="165dp"
        android:layout_height="127dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.385"
        tools:progress="0" />

    <TextView
        android:id="@+id/text_view_carbs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="44dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/carbs_progress"
        app:layout_constraintEnd_toEndOf="@+id/carbs_progress"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/carbs_progress"
        app:layout_constraintTop_toTopOf="@+id/carbs_progress"
        app:layout_constraintVertical_bias="0.018"
        tools:text="0%" />

    <ProgressBar
        android:id="@+id/protein_progress"
        style="@style/CircularDeterminateProgressBar"
        android:layout_width="165dp"
        android:layout_height="127dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.214"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.663"
        tools:progress="0" />

    <TextView
        android:id="@+id/text_view_protein"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="44dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="@+id/protein_progress"
        app:layout_constraintEnd_toEndOf="@+id/protein_progress"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="@+id/protein_progress"
        app:layout_constraintTop_toTopOf="@+id/protein_progress"
        app:layout_constraintVertical_bias="0.018"
        tools:text="0%" />


</androidx.constraintlayout.widget.ConstraintLayout>